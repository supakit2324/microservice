{
  "name": "microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently -c \"cyan.bold,red.bold\" \"npm run dev-users\" \"npm run dev-books\" \"npm run dev-web\" \"npm run dev-dashboard\"",
    "dev-users": "nest start --watch users-microservice",
    "dev-dashboard": "nest start --watch dashboard-microservice",
    "dev-books": "nest start --watch books-microservice",
    "dev-web": "nest start --watch web-microservice",
    "start:debug-users": "nest start --debug --watch users-microservice",
    "start:debug-books": "nest start --debug --watch books-microservice",
    "start:debug-web": "nest start --debug --watch web-microservice",
    "start:debug-dashboard": "nest start --debug --watch dashboard-microservice",
    "start:prod-users": "node dist/apps/users-microservice/main",
    "start:prod-books": "node dist/apps/books-microservice/main",
    "start:prod-web": "node dist/apps/web-microservice/main",
    "start:prod-dashboard": "node dist/apps/dashboard-microservice/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e-users": "jest --config ./apps/users-microservice/test/jest-e2e.json",
    "test:e2e-books": "jest --config ./apps/books-microservice/test/jest-e2e.json",
    "test:e2e-web": "jest --config ./apps/web-microservice/test/jest-e2e.json",
    "test:e2e-dashboard": "jest --config ./apps/dashboard-microservice/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^8.2.2",
    "dayjs": "^1.11.10",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}